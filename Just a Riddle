@client.event
async def on_member_join(ctx, member):
  channel = client.get_channel("... --- ..-. .-")
  rules = client.get_channel("Nothing")
  msg = "Welcome visitor {}\n read the {}".format(member.mention, rules.mention)
  await ctx.send_message(channel, msg)
  
